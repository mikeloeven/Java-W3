
public class Master {
	
	public static void main(String[] args)
	{
		
		
		
			Account A1122 = new Account(1122, 20000, 4.5);
			System.out.println("New Account ID: " +A1122.getId()+ " :Created On: "+A1122.getDateCreated().toString()+" : With a Ballance Of: $"+A1122.getBallance()+" :And a Annual Interest rate Of: "+A1122.getAnnualInterestRate()+"%");
			
			A1122.withdraw(2500);
			A1122.deposit(3000);
		
			System.out.println("Account Staus for ID: "+A1122.getId()+" \n The Ballance is: $"+A1122.getAnnualInterestRate()+"\n The Monthly interest Is : $"+A1122.getMonthlyInterest()+"\n This Account was Opened On: "+A1122.getDateCreated());
	}

}
